@{
    ViewData["Title"] = "Cuchara Urbana Street Food";

    }

<body style="background-color:#FFE4B5">
<div class="text-center">
    <h1 class="display-4" style="color: red"><b>Bienvenidos a Cuchara Urbana Street Food</b></h1>
    <p></p>
    <p></p>
    <p></p>
</div>


<div id="myCarousel" class="carousel slide" data-ride="carousel" style="margin-top:-15px; background-color:black">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>

    <div class="carousel-inner" role="listbox">
        <div class="carousel-item active">
                <img class="d-block w-100" style=" height: 25%; width: 25%;" src="~/images/Melted.jpg" alt="Img2" />
            <div class="carousel-caption">
                <p>¡Melt Callejero!</p>     
            </div>
        </div>
        <div class="carousel-item">
                <img class="d-block w-100" style="height: 300px; width: 50%;" src="~/images/Urban.jpg" alt="Img3" />
            <div class="carousel-caption">
                <p>Urban Classic</p>
            </div>
        </div>
        <div class="carousel-item">
                <img class="d-block w-100" style="height: 300px; width: 50%;" src="~/images/Krunchy.jpg" alt="Img4" />
            <div class="carousel-caption">
                <p>Krunchy Bella</p>
            </div>
        </div>
        <div class="carousel-item">
                <img class="d-block w-100" style="height: 300px; width: 50%;" src="~/images/Juicy.jpg" alt="Img4" />
            <div class="carousel-caption">
                <p>Juicy Luchi</p>
            </div>
        </div>
    </div>
    <a class="carousel-control-prev" href="#myCarousel" role="button" data-slide="prev">
        <span class="sr-only">Anterior</span>
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        
    </a>
    <a class="carousel-control-next" href="#myCarousel" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Siguiente</span>
    </a>
</div>
</body>




